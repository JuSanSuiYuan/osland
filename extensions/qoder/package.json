{
  "name": "osland-qoder-extension",
  "displayName": "OSland for Qoder",
  "description": "OSland Visual Editor Extension for Qoder IDE",
  "version": "0.1.0",
  "publisher": "osland-project",
  "engines": {
    "qoder": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:osland.openVisualEditor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "osland.openVisualEditor",
        "title": "Open OSland Visual Editor",
        "icon": {
          "dark": "./media/icon-dark.svg",
          "light": "./media/icon-light.svg"
        }
      },
      {
        "command": "osland.saveProject",
        "title": "Save OSland Project"
      },
      {
        "command": "osland.loadProject",
        "title": "Load OSland Project"
      },
      {
        "command": "osland.runProject",
        "title": "Run OSland Project"
      },
      {
        "command": "osland.buildProject",
        "title": "Build OSland Project"
      }
    ],
    "keybindings": [
      {
        "command": "osland.openVisualEditor",
        "key": "ctrl+shift+o",
        "mac": "cmd+shift+o"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "osland-explorer",
          "title": "OSland",
          "icon": "./media/icon.svg"
        }
      ]
    },
    "views": {
      "osland-explorer": [
        {
          "type": "webview",
          "id": "osland.canvas",
          "name": "Canvas",
          "icon": "./media/canvas.svg"
        },
        {
          "type": "webview",
          "id": "osland.components",
          "name": "Components",
          "icon": "./media/components.svg"
        },
        {
          "type": "webview",
          "id": "osland.properties",
          "name": "Properties",
          "icon": "./media/properties.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.67.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "license": "MulanPSL-2.0"
}
